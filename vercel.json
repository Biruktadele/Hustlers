{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.13"
  },
  "functions": {
    "app/main.py": {
      "runtime": "python3.13"
    }
  },
  "crons": [
    {
      "path": "/api/sync-jobs",
      "schedule": "0 0 * * *"
    }
  ]
}
